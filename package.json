{
  "name": "notion-api",
  "version": "0.0.0",
  "description": "Unofficial TypeScript Notion API.",
  "type": "module",
  "main": "dist/app.js",
  "author": {
    "name": "Wynne"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yowynn/notion-api"
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "start": "npm run build && node ."
